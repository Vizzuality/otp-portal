{
  "plugins": [
    "transform-decorators-legacy",
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "components": "./components",
          "constants": "./constants",
          "css": "./css",
          "modules": "./modules",
          "selectors": "./selectors",
          "services": "./services",
          "utils": "./utils"
        },
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "presets": [
    "next/babel"
  ],
  "ignore": []
}
